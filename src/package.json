{
    "name": "chicken-service",
    "version": "1.0.0",
    "description": "Chicken Service for NodeJS",
    "main": "app.js",
    "repository": "",
    "author": "SAM CLI",
    "license": "MIT",
    "scripts": {
        "start": "cd.. && sam local start-api --docker-network lambda-local --skip-pull-image",
        "start-dynamo": "cd.. && docker-compose up -d",
        "build": "cd.. && sam build --beta-features",
        "unit": "jest",
        "lint": "eslint --fix '**/*.ts'",
        "compile": "tsc",
        "deploy": "cd.. && sam deploy --profile default",
        "test": "npm run compile && npm run unit",
        "stop-dynamo": "cd.. && docker-compose down"
    },
    "devDependencies": {
        "@aws-sdk/client-dynamodb": "^3.118.0",
        "@types/aws-lambda": "^8.10.92",
        "@types/jest": "^27.5.2",
        "@types/node": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^5.10.2",
        "esbuild": "^0.14.14",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.0",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    }
}
