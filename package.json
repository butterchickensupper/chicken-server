{
  "name": "chicken-server",
  "version": "1.0.0",
  "description": "TF + Docker + AWS + DynamoDB",
  "main": "index.js",
  "scripts": {
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down && docker-compose rm",
    "tf:init": "cd terraform && terraform init",
    "tf:check": "cd terraform && terraform fmt -check",
    "tf:validate": "cd terraform && terraform validate",
    "tf:plan": "tsc && cd terraform && terraform plan -out=myplan -var-file=terraform.tfvars",
    "tf:apply": "cd terraform && terraform apply -auto-approve myplan",
    "tf:destroy": "cd terraform && terraform destroy -auto-approve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/butterchickensupper/chicken-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/butterchickensupper/chicken-server/issues"
  },
  "homepage": "https://github.com/butterchickensupper/chicken-server#readme",
  "dependencies": {
    "aws-sdk": "^2.1148.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "typescript": "^4.7.3"
  }
}
